<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.plugin.cleardata.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Clear Data Test</name>
    <description>
        An Cordova project to test/validate cordova-plugin-clear-data
    </description>
    <author email="dave@workingedge.co.uk" href="http://www.workingedge.co.uk">Dave Alden</author>
    <content src="index.html" />

    <access origin="*"/>
    <allow-navigation href="*"/>
    <allow-intent href="*"/>

    <engine name="android" spec="^6.3.0" />
    <platform name="android">
    </platform>

    <engine name="ios" spec="^4.5.4" />
    <platform name="ios">
    </platform>

    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
</widget>
